<robot name="airobot">
    <link name="body"/>
    <link name="armA"/>
    <link name="armB"/>
    <link name="handA"/>
    <link name="handB"/>

    <joint name="jointBodyArmA" type="continuous">
        <parent link="body"/>
        <child link="armA"/>
        <origin xyz="0 -0.34 -0.44" rpy="0 0 0"/>
    </joint>

    <joint name="jointBodyArmB" type="continuous">
        <parent link="body"/>
        <child link="armB"/>
        <origin xyz="0 0.34 -0.49" rpy="0 0 0"/>
    </joint>

    <joint name="jointArmHandA" type="continuous">
        <parent link="armA"/>
        <child link="handA"/>
        <origin xyz="0 -1.21 -2.21" rpy="0 0 0"/>
    </joint>

    <joint name="jointArmHandB" type="continuous">
        <parent link="armB"/>
        <child link="handB"/>
        <origin xyz="0 1.13 -2.28" rpy="0 0 0"/>
    </joint>
</robot>
