<robot name="airobot">
    <link name="drone">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://drone.dae"/>
            </geometry>
        </visual>
    </link>
 
    <link name="arm A"/>
    <link name="arm B"/>
    <link name="hand A"/>
    <link name="hand B"/>

    <joint name="jointBodyArmA" type="continuous">
        <parent link="drone"/>
        <child link="arm A"/>
        <origin xyz="0 -0.34 -0.44" rpy="0 0 0"/>
   </joint>

    <joint name="jointBodyArmB" type="continuous">
        <parent link="drone"/>
        <child link="arm B"/>
        <origin xyz="0 0.34 -0.49" rpy="0 0 0"/>
    </joint>

    <joint name="jointArmHandA" type="continuous">
        <parent link="arm A"/>
        <child link="hand A"/>
        <origin xyz="0 -1.21 -2.21" rpy="0 0 0"/>
    </joint>

    <joint name="jointArmHandB" type="continuous">
        <parent link="arm B"/>
        <child link="hand B"/>
        <origin xyz="0 1.13 -2.28" rpy="0 0 0"/>
    </joint>
</robot>
