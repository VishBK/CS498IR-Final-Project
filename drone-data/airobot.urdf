<robot name="airobot">
    <link name="drone">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="drone.dae"/>
            </geometry>
        </visual>
    </link>
 
    <link name="armA"/>
    <link name="armB"/>

    <joint name="jointBodyArmA" type="continuous">
        <parent link="drone"/>
        <child link="armA"/>
        <origin xyz="0 -0.34 -0.44" rpy="0 0 0"/>
   </joint>

    <joint name="jointBodyArmB" type="continuous">
        <parent link="drone"/>
        <child link="armB"/>
        <origin xyz="0 0.34 -0.49" rpy="0 0 0"/>
    </joint>
</robot>
